{
  "name": "@tjikko/components",
  "version": "3.4.9",
  "description": "Component library for Tjikko Studio apps",
  "main": "index.js",
  "scripts": {
    "build:analyze": "webpack --config ./webpack.analyze.js",
    "build:dev": "webpack --config ./webpack.dev.js",
    "build:prod": "webpack --config ./webpack.prod.js --node-env production",
    "build:css": "postcss ./assets/globals.css -o ./assets/app.css",
    "build:storybook": "npm run build:css && build-storybook -c .storybook -s assets",
    "build:all": "npm run build && npm run build:storybook",
    "build": "npm run build:prod && npm run build:css",
    "dev": "npm run build:dev -- --watch",
    "dev:storybook": "start-storybook -s ./assets -p 6006 --quiet",
    "start": "concurrently npm:dev npm:dev:storybook",
    "test": "echo 'No tests yet'"
  },
  "keywords": [],
  "author": "Tjikko Studio <https://tjikko.studio>",
  "license": "",
