  "description": "Component library for Tjikko Studio apps",
  "main": "index.js",
  "scripts": {
    "build:analyze": "webpack --config ./webpack.analyze.js --node-env production",
    "build:dev": "webpack --config ./webpack.dev.js --node-env development",
    "build:prod": "webpack --config ./webpack.prod.js --node-env production",
    "build:css": "postcss ./assets/globals.css -o ./assets/app.css",
    "build": "npm run build:prod && npm run build:css",
    "dev": "npm run build:dev -- --watch",
    "start": "npm run dev",
    "test": "jest --passWithNoTests",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier src --write",
    "clean": "rm -rf styles.js index.js index.js.LICENSE.txt assets/app.css",
    "uninstall": "rm -rf node_modules"
  },
