{
  "name": "@tjikko/components",
  "version": "2.1.0",
  "description": "Component library for Tjikko Studio apps",
  "main": "index.js",
  "scripts": {
    "build:dev:components": "babel components --out-dir dist --extensions \".tsx\" --ignore \"components/**/*.stories.*\" --source-maps inline",
    "build:dev:styles": "babel styles --out-dir dist/styles --extensions \".tsx\" --source-maps inline",
    "build:prod:components": "cross-env BABEL_ENV=production babel components --out-dir dist --extensions \".tsx\" --ignore \"components/**/*.stories.*\"",
    "build:prod:styles": "cross-env BABEL_ENV=production babel styles --out-dir dist/styles --extensions \".tsx\" ",
    "dev:babel:components": "npm run build:dev:styles -- --watch",
    "dev:babel:styles": "npm run build:dev:styles -- --watch",
    "dev": "concurrently npm:dev:babel:components npm:dev:babel:styles npm:dev:storybook",
    "build": "concurrently npm:build:prod:components npm:build:prod:styles",
    "test": "echo 'No tests yet'",
    "dev:storybook": "start-storybook -s ./assets -p 6006 --quiet",
    "build:storybook": "build-storybook"
  },
  "keywords": [],
  "author": "Tjikko Studio <https://tjikko.studio>",
  "license": "",
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@storybook/addon-actions": "6.3.6",
    "@storybook/addon-essentials": "6.3.6",
    "@storybook/addon-links": "6.3.6",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.3.6",
    "@svgr/webpack": "5.5.0",
    "@types/hammerjs": "2.0.40",
    "autoprefixer": "10.3.1",
    "babel-loader": "8.2.2",
    "concurrently": "6.2.0",
    "postcss": "8.3.6",
    "postcss-preset-env": "6.7.0",
    "storybook-addon-themes": "6.1.0",
    "storybook-tailwind-dark-mode": "1.0.9"
  },
  "dependencies": {
    "@headlessui/react": "1.3.0",
    "@heroicons/react": "1.0.3",
    "@types/react-slick": "0.23.5",
    "cross-env": "7.0.3",
    "hammerjs": "2.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-fast-marquee": "1.2.1",
    "react-gesture-handler": "0.0.5",
    "react-inlinesvg": "2.3.0",
    "react-media-hook": "0.4.9",
    "tailwindcss": "2.2.7"
  },
  "files": [
    "/dist/",
    "/README.md"
  ]
}